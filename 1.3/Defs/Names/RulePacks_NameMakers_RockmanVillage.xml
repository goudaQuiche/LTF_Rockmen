<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!-- Tribe -->

  <RulePackDef>
    <defName>NamerFactionRockmanVillage</defName>
    <rulePack>
      <rulesStrings>
        <li>name->The [color] [minerals] [mountains]</li>
        <li>name->The [positive] [color] [mountains]</li>
		
        <li>name->[positive] [minerals] [politicalunion]</li>
        <li>name->[color] [minerals] [terrainfeature]</li>
        <li>name->[color] [mountains] [politicalunion]</li>
		<li>name->[positive] [minerals] [terrainfeature]</li>
		
		<li>name->The [color] [minerals] [Religious]</li>
        <li>name->The [mountains] [positive] [Religious] </li>
		
        <li>name->[minerals] [Religious] [politicalunion]</li>
        <li>name->[color] [minerals] [Religious] [terrainfeature]</li>
		
		<li>name->[color] [mediumVessel] [type] [vesselRelated]</li>
		<li>name->[color] [heavyVessel] [type] [Religious]</li>
		<li>name->[heavyVessel] [type] [Religious] [politicalunion]</li>
		<!--
		<li>[Religious] [vesselRelated] [politicalunion]</li>
		<li>[color] [heavyVessel] [type] [vesselRelated]</li>
		-->
      </rulesStrings>
	  
	  <rulesRaw>
	  
		 <li Class="Rule_File">
          <keyword>lightVessel</keyword>
          <path>Vessels/light</path>
        </li>
		<li Class="Rule_File">
          <keyword>mediumVessel</keyword>
          <path>Vessels/medium</path>
        </li>
		<li Class="Rule_File">
          <keyword>heavyVessel</keyword>
          <path>Vessels/heavy</path>
        </li>
		<li Class="Rule_File">
          <keyword>type</keyword>
          <path>Vessels/type</path>
        </li>
		<li Class="Rule_File">
          <keyword>vesselRelated</keyword>
          <path>Vessels/vesselRelated</path>
        </li>

		<!-- nouns -->
		<li Class="Rule_File">
          <keyword>politicalunion</keyword>
          <path>Words/Nouns/PoliticalUnions_Outlander</path>
        </li>
        <li Class="Rule_File">
          <keyword>mountains</keyword>
          <path>RockmanNames/Mountains</path>
        </li>
        <li Class="Rule_File">
          <keyword>minerals</keyword>
          <path>RockmanNames/Minerals</path>
        </li>

		<!-- adj -->
        <li Class="Rule_File">
          <keyword>color</keyword>
          <path>Words/Nouns/Colors</path>
        </li>
		<li Class="Rule_File">
          <keyword>positive</keyword>
          <path>Words/Nouns/Concepts_Positive</path>
        </li>
		<li Class="Rule_File">
          <keyword>Religious</keyword>
          <path>Location/Religious</path>
        </li>
		
      </rulesRaw>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerBaseRockmanVillage</defName>
    <rulePack>
      <rulesStrings>
		<li>name->[color] [terrainfeature] [mountains]</li>
        <li>name->[positive] [terrainfeature] of [mountains]</li>
        <li>name->[positive] [minerals] [mountains]</li>
		<li>name->[positive] [mountains] [minerals]</li>
        <li>name->[color] [minerals] [terrainfeature]</li>
        <li>name->[positive] [terrainfeature] [minerals]</li>
		<li>name->The [color] [mountains] [minerals]</li>
		
		<li>name->[color] [mountains] [Religious]</li>
		<li>name->[color] [positive] [Religious]</li>
		<li>name->[Minerals] [Mountains] [Religious]</li>
		<li>name->[Mountains] [positive] [Religious]</li>
		<li>name->[positive] [Religious] [Mountains]</li>
		<li>name->[color] [Religious] [Mountains]</li>
		
		<li>name->[color] [mediumVessel] [type] [vesselRelated]</li>
		<li>name->[heavyVessel] [type] [terrainfeature] [Religious]</li>
		<li>name->[heavyVessel] [type] [Religious] [politicalunion]</li>
      </rulesStrings>
	  
	  <rulesRaw>
		 <li Class="Rule_File">
          <keyword>lightVessel</keyword>
          <path>Vessels/light</path>
        </li>
		<li Class="Rule_File">
          <keyword>mediumVessel</keyword>
          <path>Vessels/medium</path>
        </li>
		<li Class="Rule_File">
          <keyword>heavyVessel</keyword>
          <path>Vessels/heavy</path>
        </li>
		<li Class="Rule_File">
          <keyword>type</keyword>
          <path>Vessels/type</path>
        </li>
		<li Class="Rule_File">
          <keyword>vesselRelated</keyword>
          <path>Vessels/vesselRelated</path>
        </li>
<!-- noun -->
		<li Class="Rule_File">
          <keyword>politicalunion</keyword>
          <path>Words/Nouns/PoliticalUnions_Outlander</path>
        </li>
        <li Class="Rule_File">
          <keyword>mountains</keyword>
          <path>RockmanNames/Mountains</path>
        </li>
        <li Class="Rule_File">
          <keyword>minerals</keyword>
          <path>RockmanNames/Minerals</path>
        </li>
<!-- Adj -->
		<li Class="Rule_File">
          <keyword>Religious</keyword>
          <path>Location/Religious</path>
        </li>
        <li Class="Rule_File">
          <keyword>color</keyword>
          <path>Words/Nouns/Colors</path>
        </li>
		
      </rulesRaw>
    </rulePack>
  </RulePackDef>

</Defs>
