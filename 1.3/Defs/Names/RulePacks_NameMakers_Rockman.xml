<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerPersonRockman</defName>
    <rulePack>
      <rulesStrings>

		

		<li>name->[CompassDirections] '[Large][Roman]' [RegularRocks]</li>
		<li>name->[Large] '[Cardinal][Natural]' [RegularRocks]</li>
		<li>name->[Angsty] '[CompassDirections] [Color]' [RegularRocks]</li>
		<li>name->[CompassDirections] '[GreekAlphabet][Large]' [RegularRocks]</li>
		<li>name->[Large] '[GreekAlphabet][RegularRocks]' [minerals]</li>
		<li>name->[Religious] '[Minerals][NaturalObject]' [RegularRocks]</li>
		
		<li>name->[Caves] '[RegularRocks] [Roman]' [Mountains]</li>
		<li>name->[Angsty] '[Large] [RegularRocks]' [Mountains]</li>		
		<li>name->[Religious] '[minerals] [Roman]' [Mountains]</li>
        <li>name->[Angsty] '[RegularRocks][NaturalObject]' [Caves]</li>

        <li>name->[Large] '[Natural] [RegularRocks]' [Caves]</li>
		
      </rulesStrings>
	  
      <rulesRaw>
	  <!-- mod -->
		<!-- noun -->
        <li Class="Rule_File">
          <keyword>Mountains</keyword>
          <path>RockmanNames/Mountains</path>
        </li>
        <li Class="Rule_File">
          <keyword>Minerals</keyword>
          <path>RockmanNames/Minerals</path>
        </li>
		<li Class="Rule_File">
          <keyword>RegularRocks</keyword>
          <path>RockmanNames/RegularRocks</path>
        </li>

		<li Class="Rule_File">
          <keyword>Caves</keyword>
          <path>Location/Caves</path>
        </li>
		<li Class="Rule_File">
          <keyword>Religious</keyword>
          <path>Location/Religious</path>
        </li>
		<!-- misc -->
		<li Class="Rule_File">
          <keyword>GreekAlphabet</keyword>
          <path>Common/GreekAlphabet</path>
        </li>
		<li Class="Rule_File">
          <keyword>Cardinal</keyword>
          <path>Common/GreekCardinalPrefixes</path>
        </li>
		
		<!-- vanilla		 -->
			<!-- noun -->
        <li Class="Rule_File">
			<keyword>NaturalObject</keyword>
			<path>Words/Nouns/NaturalObject</path>
        </li>
        <li Class="Rule_File">
			<keyword>Color</keyword>
			<path>Words/Nouns/Colors</path>
        </li>
			<!-- adj -->
		<li Class="Rule_File">
			<keyword>CompassDirections</keyword>
			<path>Words/Adjectives/CompassDirections</path>
        </li>
		<li Class="Rule_File">
			<keyword>Angsty</keyword>
			<path>Words/Adjectives/Angsty</path>
        </li>
		<li Class="Rule_File">
			<keyword>Large</keyword>
			<path>Words/Adjectives/Large</path>
		</li>
		<li Class="Rule_File">
			<keyword>Natural</keyword>
			<path>Words/Adjectives/Natural</path>
        </li>
			<!-- misc -->
		<li Class="Rule_File">
			<keyword>Roman</keyword>
			<path>Words/Misc/Numbers_SmallRoman</path>
        </li>

		
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  
</Defs>